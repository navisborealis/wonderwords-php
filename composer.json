{
  "name": "navisborealis/wonderwords-php",
  "description": "Generate random words and sentences with ease in PHP.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "adjective",
    "noun",
    "word",
    "generator",
    "phrase"
  ],
  "homepage": "https://github.com/navisborealis/wonderwords-php",
  "require": {
    "php": "^7.1 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "NavisBorealis\\WonderwordsPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NavisBorealis\\WonderwordsPhp\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Piotr Grabski-Gradzi≈Ñski",
      "email": "piotr.gradzinski@navisborealis.io"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "php-coveralls/php-coveralls": "dev-master",
    "phpunit/phpunit": "^7.5 || ^8.5 || ^9.6",
    "phpstan/phpstan": "^1.4",
    "friendsofphp/php-cs-fixer": "^3.1"
  }
}
